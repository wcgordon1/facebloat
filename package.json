{
  "name": "convex-saas",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "convex dev",
    "predev": "convex dev --run init --until-success",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ignore-pattern convex/_generated --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc -p tsconfig.app.json && tsc -p tsconfig.node.json && tsc -p convex/tsconfig.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@auth/core": "^0.34.1",
    "@convex-dev/auth": "^0.0.43",
    "@convex-dev/react-query": "^0.0.0-alpha.3",
    "@epic-web/client-hints": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@react-email/components": "0.0.22",
    "@tanstack/react-form": "0.26.4",
    "@tanstack/react-query": "^5.51.15",
    "@tanstack/react-router": "^1.45.11",
    "@tanstack/zod-form-adapter": "^0.26.4",
    "@xixixao/uploadstuff": "^0.0.5",
    "class-variance-authority": "^0.7.0",
    "convex": "^1.13.2",
    "convex-helpers": "^0.1.50",
    "cookie": "^0.6.0",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "lucide-react": "^0.416.0",
    "npm-run-all": "^4.1.5",
    "oslo": "^1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.0.0",
    "resend": "^3.2.0",
    "stripe": "^16.6.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@tanstack/router-devtools": "^1.45.11",
    "@tanstack/router-plugin": "^1.45.8",
    "@types/cookie": "^0.6.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.40",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.3.4"
  }
